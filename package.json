  "license": "MIT",
  "keywords": [
    "anime",
    "download",
    "crunchyroll"
  ],
  "name": "crunchy",
  "repository": {
    "type": "git",
    "url": "git://github.com/Godzil/crunchyroll.js.git"
  },
  "engines": {
    "node": ">=5.0"
  },
  "version": "1.1.20",
  "bin": {
    "crunchy": "./bin/crunchy"
  },
  "dependencies": {
